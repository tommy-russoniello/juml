<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.CheckBox?>

<Pane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="juml.LineController">
	<Label prefHeight="29.0" prefWidth="100.0" text=" Start Origin X" />
	<TextField fx:id="lineStartOriginX" layoutX="100.0" layoutY="-1.0" />
	<Label layoutY="29.0" prefHeight="32.0" prefWidth="100.0" text=" Start Origin Y" />
	<TextField fx:id="lineStartOriginY" layoutX="100.0" layoutY="30.0" />
	<Label layoutY="61.0" prefHeight="32.0" prefWidth="100.0" text=" End Origin X" />
	<TextField fx:id="lineEndOriginX" layoutX="100.0" layoutY="61.0" />
	<Label layoutY="92.0" prefHeight="32.0" prefWidth="100.0" text=" End Origin Y" />
	<TextField fx:id="lineEndOriginY" layoutX="100.0" layoutY="92.0" />
	<Separator layoutY="125.0" prefHeight="3.0" prefWidth="298.0" />
    <CheckBox fx:id="lineDotted" layoutX="14.0" layoutY="128.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="120.0" text="Dotted Line"  selected="false"  onMouseClicked="#lineStatusToggle"/>
</Pane>
