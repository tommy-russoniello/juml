   	<TextField fx:id="relationshipEndOriginX" layoutX="100.0" layoutY="61.0" />
   	<Label layoutY="92.0" prefHeight="32.0" prefWidth="100.0" text=" End Origin Y" />
   	<TextField fx:id="relationshipEndOriginY" layoutX="100.0" layoutY="92.0" />
